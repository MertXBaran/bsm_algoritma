    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="dizi" type="Integer" array="True" size="10"/>
            <declare name="i, j, temp" type="Integer" array="False" size=""/>
            <for variable="i" start="0" end="9" direction="inc" step="1">
                <output expression="i+1 &amp; &quot;. Elemana De&#287;er Atay&#305;n&quot;" newline="True"/>
                <input variable="dizi[i]"/>
            </for>
            <for variable="i" start="0" end="8" direction="inc" step="1">
                <for variable="j" start="i+1" end="9" direction="inc" step="1">
                    <if expression="dizi[i] &gt; dizi[j]">
                        <then>
                            <assign variable="temp" expression="dizi[i]"/>
                            <assign variable="dizi[i]" expression="dizi[j]"/>
                            <assign variable="dizi[j]" expression="temp"/>
                        </then>
                        <else/>
                    </if>
                </for>
            </for>
            <for variable="i" start="0" end="9" direction="inc" step="1">
                <output expression="dizi[i]" newline="True"/>
            </for>
        </body>
    </function>
