                <if expression="b[i]&gt;enbuyuksayi">
                    <then>
                        <assign variable="sayiindex" expression="i"/>
                        <assign variable="enbuyuksayi" expression="b[i]"/>
                    </then>
                    <else/>
                </if>
            </for>
            <output expression="&quot;Girilen en b&#252;y&#252;k say&#305;: &quot; &amp; enbuyuksayi &amp; &quot;, dizinin &quot; &amp; (sayiindex + 1) &amp; &quot;. eleman&#305;d&#305;r ve dizideki index numaras&#305;: &quot; &amp; sayiindex &amp; &quot; olarak belirlenmi&#351;tir.&quot;" newline="True"/>
        </body>
    </function>
