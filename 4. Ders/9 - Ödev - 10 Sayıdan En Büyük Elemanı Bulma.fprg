    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="b" type="Integer" array="True" size="10"/>
            <declare name="i, enbuyuksayi, sayiindex" type="Integer" array="False" size=""/>
            <assign variable="enbuyuksayi" expression="0"/>
            <for variable="i" start="0" end="9" direction="inc" step="1">
                <output expression="i+1 &amp; &quot;. Elemana De&#287;er Atay&#305;n&quot;" newline="True"/>
